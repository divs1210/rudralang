ns!(fizzbuzz,
  {} =>

  let fizzbuzz := fn([n] =>
    cond(
      zero?(mod(n, 15)) => "FizzBuzz"
      zero?(mod(n, 5))  => "Buzz"
      zero?(mod(n, 3))  => "Fizz"
      :else             => n
    )
  )

  defn!(main!, {}, [_, n] =>
    let n := string->number(n)

    for-each(
      fn([x] => println!(fizzbuzz(x))),
      range(1, inc(n), 1)
    )
  )
)
