ns!(fizzbuzz,
  {} =>

  defn!(fizzbuzz, {}, [n] =>
    cond(
      zero?(mod(n, 15)) => "FizzBuzz"
      zero?(mod(n, 5))  => "Buzz"
      zero?(mod(n, 3))  => "Fizz"
      :else             => n
    )
  )

  defn!(main!, {}, [_, n] =>
    for-each(
      fn([x] => println!(fizzbuzz(x))),
      range(1, add(1, string->number(n)))
    )
  )
)
